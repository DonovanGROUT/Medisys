# Template de configuration pour les tests
# Copier vers .env.test et modifier les valeurs

# define your env variables for the test env here
KERNEL_CLASS='App\Kernel'
APP_SECRET='your_test_secret_here'

# Configuration base de données pour les tests
# Utilise MySQL avec une base dédiée aux tests
# Note: Symfony ajoute automatiquement le suffixe "_test" à la base
DATABASE_URL="mysql://user:password@127.0.0.1:${MYSQL_PORT}/database_name?charset=utf8mb4&serverVersion=8.0"
